<ul class="list-unstyled ml-5 mt-3 position-relative zindex-1">
  <%social_settings.each do |social_setting|%>
    <li class="mb-3">
      <%= link_to raw("<i class=\"#{social_setting[:icon_class]}\"></i>"), generate_social_url(social_setting), class: 'text-white', target: '_blank' %>
    </li>
  <% end %>
</ul>
